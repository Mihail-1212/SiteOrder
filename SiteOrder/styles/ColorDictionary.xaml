<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:SiteOrder.styles">


    <SolidColorBrush Color="#2d3436" x:Key="darkula"/>
    
    <SolidColorBrush Color="#bcc9cf" x:Key="title_button_active"/>
    <SolidColorBrush Color="#bcc9cf" x:Key="title_button_hover" Opacity="0.7"/>

    <SolidColorBrush Color="Red" x:Key="red_hover"/>
    <SolidColorBrush Color="Red" x:Key="red_active" Opacity="0.7"/>

    <SolidColorBrush Color="#81ecec" x:Key="faded_poster" Opacity="0.5"/>
    <SolidColorBrush Color="#74b9ff" x:Key="green_darnel_tail" Opacity="1"/>
    
    <SolidColorBrush Color="#0984e3" x:Key="electron_blue" Opacity="1"/>
    <SolidColorBrush Color="{Binding Path=Color, Source={StaticResource electron_blue}}" x:Key="electron_blue_hover" Opacity="0.7"/>

    <SolidColorBrush Color="#dfe6e9" x:Key="MainWindowColor"/>

    <SolidColorBrush Color="#7f8fa6" x:Key="PlaceHolderColor"/>

    <SolidColorBrush Color="#3498db" x:Key="PeterReaver"/>

</ResourceDictionary>